
<%- include("header") -%>

  <div class="box" id="heading">
    <h1>It's a <%= listTitle %> !</h1>
  </div>

  <div class="box">



  

  <%   newListItems.forEach(function(singeItem){ %>
      <div class="item">
        <input type="checkbox">
